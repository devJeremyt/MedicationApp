<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="600.0" minWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.westga.cs3211.prescription_app.view.ViewSideEffects">
   <children>
      <ListView fx:id="sideEffectListView" layoutX="14.0" layoutY="57.0" prefHeight="486.0" prefWidth="374.0" />
      <Button fx:id="addButton" layoutX="47.0" layoutY="21.0" mnemonicParsing="false" onAction="#openNewSideEffectDialog" text="Add">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="updateButton" layoutX="289.0" layoutY="21.0" mnemonicParsing="false" onAction="#openUpdateSideEffectDialog" text="Update">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="removeButton" layoutX="165.0" layoutY="21.0" mnemonicParsing="false" onAction="#removeSideEffect" text="Remove">
         <font>
            <Font size="14.0" />
         </font>
         </Button>
      <Button fx:id="viewSideEffectButton" layoutX="152.0" layoutY="556.0" mnemonicParsing="false" onAction="#openViewSideEffectPopUp" text="View Details">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
