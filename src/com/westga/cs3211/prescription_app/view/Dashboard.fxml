   <children>
      <ListView fx:id="prescriptionListView" layoutX="14.0" layoutY="57.0" prefHeight="494.0" prefWidth="374.0" />
      <Button fx:id="addButton" layoutX="50.0" layoutY="21.0" mnemonicParsing="false" onAction="#addPrescription" text="Add">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="updateButton" layoutX="283.0" layoutY="21.0" mnemonicParsing="false" text="Update">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="viewDetailsButton" layoutX="152.0" layoutY="556.0" mnemonicParsing="false" text="View Details">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
